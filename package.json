{
  "name": "changeloger",
  "version": "0.0.0",
  "description": "A fast and lightweight changelog generator using conventional commits",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "bin": {
    "changeloger": "./dist/cli.mjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "dev": "node --experimental-specifier-resolution=node --loader ts-node/esm ./src/bin/changeloger.ts",
    "build": "rollup --config",
    "test": "node dist/cli.mjs"
  },
  "keywords": [],
  "author": "trandaison",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.9",
    "@types/yargs": "^17.0.32",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.11",
    "defu": "^6.1.4",
    "fs-extra": "^11.2.0",
    "mri": "^1.2.0",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "yargs": "^17.7.2"
  }
}
